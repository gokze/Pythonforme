class Solution(object):
    def removeDuplicates(self, nums):
        l = len(nums)
        if l <= 2:
            return l  

        k = 2  
        for i in range(2, l):
            if nums[i] != nums[k - 2]:  
                nums[k] = nums[i]
                k += 1  

        print(nums[:k])
        return k
