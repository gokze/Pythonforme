class Solution(object):
    def plusOne(self, digits):
        c=0
        y=[]
        k=len(digits)-1
        for i in digits:
            c=c+i*pow(10,k)
            k-=1
        c+=1
        print(c)
        for i in range(0,len(str(c))):
            y.append(c%10)
            c//=10
        print(y)
        y.reverse()
        return y
        
