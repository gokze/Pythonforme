class Solution(object):
    def isAnagram(self, s, t):
        if len(s)!=len(t):
            return False
        count1={}
        count2={}
        for ch2 in t:

            if ch2 not in count2:
                count2[ch2]=1
            else:
                count2[ch2]+=1
        for ch1 in s:

            if ch1 not in count1:
                count1[ch1]=1
            else:
                count1[ch1]+=1
            if ch1 not in t or count1[ch1]>count2[ch1]:
                return False
        return True
        
