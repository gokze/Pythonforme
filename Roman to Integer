class Solution(object):
    def romanToInt(self, s):
        sum=0
        x={
            "I":1,
            "V":5,
            "X":10,
            "L":50,
            "C":100,
            "D":500,
            "M":1000
        }  
        y=list(s)
        for i in range(0,len(y)):
            if i<len(y)-1 and x[y[i+1]]>x[y[i]]:
                sum=sum-x[y[i]]
            else:
                sum=sum+x[y[i]]
        return sum
