// Online C compiler to run C program online
#include <stdio.h>
#include <stdlib.h>
struct node{
    int data;
    struct node *right;
    struct node *left;
};
struct node *create(){
    int val;
    struct node *newnode=malloc(sizeof(struct node));
    printf("ENTER NEW NODE DATA: ");
    scanf("%d",&val);
    if(val==-1){
        return 0;
    }
    newnode->data=val;
    printf("ENTER THE LEFT NODE OF %d:",val);
    newnode->left=create();
    printf("ENTER THE RIGHT NODE OF %d:",val);
    newnode->right=create();
    return newnode;
}
int main() {
    struct node *head=NULL;
    head=create();
    printf("%d",head->left->right->data);
}
