class Solution(object):
    def isIsomorphic(self, s, t):
        s_t={}
        t_s={}
        if len(s)!=len(t):
            return False
        for ch1,ch2 in zip(s,t):
            if ch1 in s_t and s_t[ch1]!=ch2:
                return False
            if ch2 in t_s and t_s[ch2]!=ch1:
                return False

            s_t[ch1]=ch2
            t_s[ch2]=ch1
        return True                 
