class Solution(object):
    def isPalindrome(self, x):
        y=[]
        j=c=0
        i=x
        k=len(str(x))
        if i<0:
            return False
        while i>0:
            y.append(i%10)
            j+=1
            i=i//10
        for j in str(x):
            if int(j)==x%10:
                c+=1
            x=x//10
        print(y)
        print(c)
        if c==k:
            return True
        else:
            return False

